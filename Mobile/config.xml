<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sap.myhealthcare1" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" 	xmlns:gap="http://phonegap.com/ns/1.0" 
	xmlns:android="http://schemas.android.com/apk/res/android"
>
    <name>My HealthCare</name>
    <description>
    	Application for mobile devices to enable caregivers and patients to record and upload data on their ALS condition
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
	<icon src="www/img/logo.png"/>
	<gap:config-file platform="android" parent="/manifest">
		<application android:debuggable="true" />
	</gap:config-file>

</widget>
